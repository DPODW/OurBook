<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ourbook.shop.mapper.MemberMapper">

    <insert id="sellerInsert">
        insert into seller_tb(sellerName,sellerId,sellerPwd,sellerEmail,sellerRole)values(#{name},#{id},#{pwd},#{email},#{role});
    </insert>

    <insert id="buyerInsert">
        insert into buyer_tb(name,email,role)values(#{name},#{email},#{role});
    </insert>

</mapper>